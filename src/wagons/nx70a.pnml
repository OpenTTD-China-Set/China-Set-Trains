random_switch (FEAT_TRAINS, SELF, random_nx70a_colour) {
    1: switch_nx70a_black_all_cargo; // waiting for more colours
}

switch (FEAT_TRAINS, SELF, switch_nx70a_articulated_part, extra_callback_info1) {
    1..2: return nx70a;
    return CB_RESULT_NO_MORE_ARTICULATED_PARTS;
}

switch (FEAT_TRAINS, SELF, switch_nx70a_graphics, position_in_articulated_veh % 3) {
    1: random_nx70a_colour;
    empty_sprites;
}

switch (FEAT_TRAINS, SELF, switch_nx70a_capacity_cargo, cargo_type_in_veh) {
    MAIL: return 120;
    GOOD: return 90;
    return 60;
}

switch (FEAT_TRAINS, SELF, switch_nx70a_capacity, position_in_articulated_veh % 3) {
    1: switch_nx70a_capacity_cargo;
    return 0;
}

switch (FEAT_TRAINS, SELF, switch_nx70a_loading_speed, cargo_type_in_veh) {
    MAIL: return 15;
    GOOD: return 15;
    return 10;
}


item (FEAT_TRAINS, nx70a) {
    property {
        // Menu
        name:                           string(STR_NAME_NX70A);
        sprite_id:                      SPRITE_ID_NEW_TRAIN;
        misc_flags:                     bitmask(TRAIN_FLAG_AUTOREFIT);
        variant_group:					3095;

        // Availability
        climates_available:             ALL_CLIMATES;
        introduction_date:              date(2009,06,01);
        model_life:                     VEHICLE_NEVER_EXPIRES;
        vehicle_life:                   30;
        track_type:                     SACN;

        // Cargo
        refittable_cargo_classes:       bitmask(CC_MAIL, CC_EXPRESS, CC_BULK, CC_PIECE_GOODS, CC_COVERED, CC_OVERSIZED, CC_LIQUID, CC_HAZARDOUS);
        non_refittable_cargo_classes:   bitmask(CC_PASSENGERS);
        cargo_allow_refit:              [];
        cargo_disallow_refit:           [YETI, YETY];
        refit_cost:                     0;
        default_cargo_type:             DEFAULT_CARGO_FIRST_REFITTABLE;
        ai_special_flag:                AI_FLAG_CARGO;
        cargo_capacity:                 70;
        loading_speed:                  10 << param_loading_speed;
        cargo_age_period:               185 << param_cargo_decay;

        // Stats
        speed:                          120 km/h;
        power:                          0 hp;
        weight:                         23.8 ton;
        cost_factor:                    8;
        running_cost_base:              RUNNING_COST_ELECTRIC;
        running_cost_factor:            6;
    }
    graphics {
        // Menu
        additional_text:                string(STR_DESC_2, string(STR_NX70A_LIVERY_AVAILABILITY), string(STR_DECAY_185));
        can_attach_wagon:               locowagon;

        // Graphics
        default:                        switch_nx70a_graphics;
