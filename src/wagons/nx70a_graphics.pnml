// start alternative colour nx70a_black
random_switch (FEAT_TRAINS, SELF, switch_nx70a_black_container_half) {
    1: spriteset_nx70a_black_container_msc_livery_half;
    1: spriteset_nx70a_black_container_half;
    1: spriteset_nx70a_black_long_container_zih_livery;
    1: spriteset_nx70a_black_long_container_xiangang_livery;
    1: spriteset_nx70a_black_long_container_evergreen_livery;
    1: spriteset_nx70a_black_long_container_maersk_livery;
    1: spriteset_nx70a_black_long_container_cr_livery;
}

random_switch (FEAT_TRAINS, SELF, switch_nx70a_black_container_full) {
    1: spriteset_nx70a_black_container_msc_livery_full;
    1: spriteset_nx70a_black_container_full;
    1: spriteset_nx70a_black_long_container_zih_livery;
    1: spriteset_nx70a_black_long_container_xiangang_livery;
    1: spriteset_nx70a_black_long_container_evergreen_livery;
    1: spriteset_nx70a_black_long_container_maersk_livery;
    1: spriteset_nx70a_black_long_container_cr_livery;
}

random_switch (FEAT_TRAINS, SELF, switch_nx70a_black_tank_container_half) {
    1: spriteset_nx70a_black_tank_container_crct_livery_half;
    1: spriteset_nx70a_black_tank_container_black_livery_half;
    1: spriteset_nx70a_black_tank_container_shanmei_livery_half;
    1: spriteset_nx70a_black_tank_container_stg_livery_half;
}

random_switch (FEAT_TRAINS, SELF, switch_nx70a_black_tank_container_full) {
    1: spriteset_nx70a_black_tank_container_crct_livery_full;
    1: spriteset_nx70a_black_tank_container_black_livery_full;
    1: spriteset_nx70a_black_tank_container_shanmei_livery_full;
    1: spriteset_nx70a_black_tank_container_stg_livery_full;
}

switch (FEAT_TRAINS, SELF, switch_nx70a_black_container_not_empty, cargo_count < 32) {
    0: switch_nx70a_black_container_full;
    switch_nx70a_black_container_half;
}

switch (FEAT_TRAINS, SELF, switch_nx70a_black_container, cargo_count < 5) {
    0: switch_nx70a_black_container_not_empty;
    spriteset_nx70a_black_empty;
}

switch (FEAT_TRAINS, SELF, switch_nx70a_black_tank_container_not_empty, cargo_count < 32) {
    0: switch_nx70a_black_tank_container_full;
    switch_nx70a_black_tank_container_half;
}

switch (FEAT_TRAINS, SELF, switch_nx70a_black_tank_container, cargo_count < 5) {
    0: switch_nx70a_black_tank_container_not_empty;
    spriteset_nx70a_black_empty;
}

random_switch (FEAT_TRAINS, SELF, switch_nx70a_black_open_container_tarpaulin) {
    1: spriteset_nx70a_black_open_container_cr_livery_tarpaulin;
// More liveries waiting to be added
}

switch (FEAT_TRAINS, SELF, switch_nx70a_black_covered_bulk, cargo_count < 5) {
    0: switch_nx70a_black_open_container_tarpaulin;
    spriteset_nx70a_black_empty;
}

switch (FEAT_TRAINS, SELF, switch_nx70a_black_cargo, cargo_classes) {
    bitmask(CC_PIECE_GOODS): switch_nx70a_black_container;
    bitmask(CC_EXPRESS): switch_nx70a_black_container;
    bitmask(CC_MAIL): switch_nx70a_black_container;
    bitmask(CC_BULK): switch_nx70a_black_open_container_tarpaulin;
    bitmask(CC_LIQUID): switch_nx70a_black_tank_container;
    spriteset_nx70a_black_cover;
}

switch (FEAT_TRAINS, SELF, switch_nx70a_black, cargo_count < 5) {
    0: switch_nx70a_black_cargo;
    spriteset_nx70a_black_empty;
}

// start cargotype metal

// metal needs extra tools to be fixed on flatbeds
// so flatbed wagon "nx70a" need alternative sprites even when it is empty
switch (FEAT_TRAINS, SELF, switch_nx70a_black_metal_not_empty, cargo_count < 32) {
    0: spriteset_nx70a_black_metal_full;
    spriteset_nx70a_black_metal_half;
}

